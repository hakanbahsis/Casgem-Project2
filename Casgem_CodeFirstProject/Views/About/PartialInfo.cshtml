@model List<Casgem_CodeFirstProject.DAL.Entities.AboutInfo>
<section class="w3l-content-with-photo-4" id="about">
    <div class="content-with-photo4-block editContent">
        <div class="container">

            @foreach (var item in Model)
            {

           
            <div class="my-bio">
                <img src="@item.ImageUrl" alt="product" class="img-responsive about-me">
                <h3 class="mt-lg-4 mt-3">@item.Title</h3>
                <p class="para mb-3">@item.Description</p>
                <p class="para mb-lg-3">@item.SubDescription</p>
            </div>
 }
        </div>
    </div>
</section>
